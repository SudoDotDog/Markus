{
    "compileOnSave": false,
    "compilerOptions": {
        "baseUrl": "./src",
        "paths": {
            "#/*": ["*"],
            "#icon/*": ["plugin/icon/*"],
            "#log/*": ["plugin/log/*"],
            "#route/*": ["service/routes/*"],
            "#global-interface": ["interface"],
            "#route-interface": ["service/interface"]
        },
        "alwaysStrict": true,
        "removeComments": true,
        "downlevelIteration": true,
        "inlineSourceMap": true,
        "jsx": "react",
        "strict": true,
        "strictNullChecks": true,
        "noImplicitReturns": true,
        "noImplicitAny": true,
        "noImplicitThis": true,
        "module": "commonjs",
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "target": "es5",
        "lib": [
            "dom",
            "es2016",
            "es6"
        ]
    },
    "exclude": [
        "node_modules",
        "dist"
    ]
}