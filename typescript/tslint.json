{
    "extends": "tslint:recommended",
    "rules": {
        "max-line-length": false,
        "new-parens": true,
        "no-arg": true,
        "no-bitwise": true,
        "no-conditional-assignment": true,
        "no-consecutive-blank-lines": false,
        "no-console": false,
        "no-eval": false,
        "no-var-requires": false,
        "object-literal-sort-keys": false,
        "prefer-const": false,
        "quotemark": false,
        "jsdoc-format": false,
        "space-before-function-paren": false,
        "variable-name": false
    },
    "jsRules": {
        "max-line-length": {
            "options": [
                128
            ]
        }
    }
}